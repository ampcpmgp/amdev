{
  "name": "amdeven-project",
  "version": "0.0.0",
  "description": "project on am development environment",
  "main": "browser/.build/start.js",
  "scripts": {
    "init": "npm i && npm run compile",
    "cl": "npm run compile && node node_modules/am-deven/app/cl",
    "start": "npm run watch",
    "start:server": "npm run compile && node app/server.js",
    "watch": "coffee -e \"(require('./am_modules/am-compiler'))::start()\"",
    "serverStart": "npm i && node app/server",
    "electron": "start ../electron-v0.36.0-win32-x64/electron.exe ./"
  },
  "devDependencies": {
    "electron-prebuilt": "latest"
  },
  "dependencies": {
    "am-autoevent": "latest",
    "am-common": "latest",
    "am-compiler": "latest",
    "am-deven": "latest",
    "am-node-parts": "latest",
    "am-template": "latest"
  },
  "author": "",
  "license": "ISC"
}
