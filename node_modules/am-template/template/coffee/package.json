{
  "name": "amdeven-project",
  "version": "0.0.0",
  "description": "project on am development environment",
  "main": "browser/lib/start.js",
  "scripts": {
    "init": "node node_modules/am-deven/app/lib/copy_template && npm i && npm run compileAll",
    "cl": "npm run compileAll && node node_modules/am-deven/app/lib/cl",
    "start": "npm run compileAll && npm i && npm run serverStart",
    "compileAll": "node node_modules/am-compiler/lib/compile_all",
    "serverStart": "node app/lib/server",
    "electron": "start ../electron-v0.33.4-win32-x64/electron.exe ./"
  },
  "dependencies": {
    "am-autoevent": "latest",
    "am-common": "latest",
    "am-compiler": "latest",
    "am-deven": "latest",
    "am-node-parts": "latest"
  },
  "author": "",
  "license": "ISC"
}
