{
  "name": "amdeven-project",
  "version": "0.0.0",
  "description": "project on am development environment",
  "main": "browser/lib/start.js",
  "scripts": {
    "init": "node -e require('am-deven/app/lib/copy_template') && npm i && coffee -c ./",
    "cl": "coffee node_modules/am-deven/app/lib/cl.coffee",
    "start": "npm run compileAll && npm i && npm run serverStart",
    "compileAll": "coffee node_modules/am-compiler/lib/compile_all.coffee",
    "serverStart": "coffee app/lib/server.coffee"
  },
  "dependencies": {
    "am-deven": "latest",
    "am-autoevent": "latest",
    "am-common": "latest",
    "am-compiler": "latest",
    "am-node-parts": "latest"
  },
  "author": "",
  "license": "ISC"
}
