    "setTemplate": "node node_modules/am-deven/test/lib/setTemplate.js && npm run electron",
    "undoTemplate": "node node_modules/am-deven/test/lib/undoTemplate.js",
    "installAll": "node test/lib/init.js",
    "TODO": ": electronのバージョンを外部から指定する（portのように）、最新を探したりする",
    "compileAll": "node node_modules/am-compiler/lib/compile_all.js",
    "electron": "start ../electron-v0.33.8-win32-x64/electron.exe ./browser/lib/start.js"
  },
