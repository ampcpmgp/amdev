{
  "name": "amdeven",
  "version": "0.1.1",
  "description": "am development enviroment",
  "main": "./app/lib/server.js",
  "scripts": {
    "init": "npm run installAll && npm run compileAll",
    "cl": "forever start node_modules/am-deven/app/lib/cl.js",
    "start": "npm run compileAll && npm run electron",
    "start:server": "npm run compileAll && npm run serverStart",
    "serverStart": "node app/lib/server.js",
    "以下気を付ける": "setTemplate->undoTemplateの順で行わないとバグ,ファイルロックは解除",
    "setTemplate": "node node_modules/am-deven/test/lib/setTemplate.js && npm run electron",
    "undoTemplate": "node node_modules/am-deven/test/lib/undoTemplate.js",
    "installAll": "node test/lib/init.js",
    "TODO": ": electronのバージョンを外部から指定する（portのように）、最新を探したりする",
    "compileAll": "coffee node_modules/am-compiler/lib/compile_all.coffee",
    "electron": "start ../electron-v0.35.1-win32-x64/electron.exe ./browser/lib/start.js",
    "electron2": "start ../electron-v0.35.1-win32-x64/electron.exe ./browser/build/start.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chokidar": "latest",
    "coffee-loader": "latest",
    "coffee-script": "latest",
    "cson": "latest",
    "data-utils": "latest",
    "fs-extra": "latest",
    "ftp": "latest",
    "is_js": "latest",
    "jquery": "latest",
    "jsdom": "latest",
    "mime": "latest",
    "mkdirp": "latest",
    "readline": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "webpack": "2.0.1-beta"
  }
}
