    "installAll": "coffee test/init.coffee",
    "TODO": ": electronのバージョンを外部から指定する（portのように）、最新を探したりする",
    "compileAll": "coffee node_modules/am-compiler/compile_all.coffee",
    "electron": "start ../electron-v0.36.0-win32-x64/electron.exe ./browser/start.js"
