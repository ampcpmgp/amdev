{
  "name": "amdeven",
  "version": "0.1.1",
  "description": "am development enviroment",
  "main": "./app/server.js",
  "scripts": {
    "init": "npm run install && npm run compile",
    "start": "npm run watch",
    "start:server": "npm run compile && node app/.build/server.js",
    "start:forever": "forever start -c coffee am_modules/am-deven/app/ci.coffee",
    "init:cloud9": "nvm install stable && nvm alias default stable",
    "TODO:消して良いかも？:": "template:set->template:undoの順で行わないとバグ,ファイルロックは解除",
    "template:set": "node node_modules/am-deven/test/setTemplate.js && npm run electron",
    "template:undo": "node node_modules/am-deven/test/undoTemplate.js",
    "_install": "npm i --prune && coffee test/init.coffee",
    "watch": "coffee -e \"(require('./am_modules/am-compiler/Base.coffee'))::start()\"",
    "compile": "coffee -e \"(require('./am_modules/am-compiler/Base.coffee'))::compile()\"",
    "electron": "electron ./browser/.build/start.js"
  },
  "dependencies": {
    "chokidar": "latest",
    "coffee-loader": "latest",
    "coffee-script": "latest",
    "cron": "latest",
    "cson": "latest",
    "cson-loader": "latest",
    "electron-prebuilt": "latest",
    "escape-html": "latest",
    "forever": "latest",
    "fs-extra": "latest",
    "ftp": "latest",
    "html-loader": "latest",
    "jquery": "latest",
    "json-loader": "latest",
    "lodash": "latest",
    "mime": "latest",
    "mkdirp": "latest",
    "node-fetch": "latest",
    "pouchdb": "latest",
    "riot": "latest",
    "riotjs-loader": "latest",
    "socket.io": "latest",
    "socket.io-client": "latest",
    "webpack": "latest"
  },
  "devDependencies": {
    "electron-prebuilt": "latest"
  },
  "author": "",
  "license": "ISC"
}
