{
  "name": "amdeven",
  "version": "0.1.1",
  "description": "am development enviroment",
  "main": "./app/lib/server.js",
  "scripts": {
    "init": "npm run installAll && npm run compileAll",
    "cl": "forever start node_modules/am-deven/app/lib/cl.js",
    "start": "npm run compileAll && npm run serverStart",
    "serverStart": "forever start app/lib/server.js",
    "serverStop": "forever stop app/lib/server.js",
